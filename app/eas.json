{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    },
    "staging": {
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "bundleIdentifier": "com.nandemocounter"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "staging",
        "EXPO_PUBLIC_ADMOB_ANDROID_APP_ID": "ca-app-pub-6862900859746528~5817361788",
        "EXPO_PUBLIC_ADMOB_IOS_APP_ID": "ca-app-pub-xxxxxxxxxxxxxxxx~yyyyyyyyyy",
        "EXPO_PUBLIC_ADMOB_BANNER_ANDROID": "ca-app-pub-6862900859746528/3358719407",
        "EXPO_PUBLIC_ADMOB_BANNER_IOS": "ca-app-pub-xxxxxxxxxxxxxxxx/yyyyyyyyyy",
        "EXPO_PUBLIC_ADMOB_INTERSTITIAL_ANDROID": "ca-app-pub-6862900859746528/5553637906",
        "EXPO_PUBLIC_ADMOB_INTERSTITIAL_IOS": "ca-app-pub-xxxxxxxxxxxxxxxx/yyyyyyyyyy"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "bundleIdentifier": "com.nandemocounter"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_ADMOB_ANDROID_APP_ID": "ca-app-pub-6862900859746528~5817361788",
        "EXPO_PUBLIC_ADMOB_IOS_APP_ID": "ca-app-pub-xxxxxxxxxxxxxxxx~yyyyyyyyyy",
        "EXPO_PUBLIC_ADMOB_BANNER_ANDROID": "ca-app-pub-6862900859746528/3358719407",
        "EXPO_PUBLIC_ADMOB_BANNER_IOS": "ca-app-pub-xxxxxxxxxxxxxxxx/yyyyyyyyyy",
        "EXPO_PUBLIC_ADMOB_INTERSTITIAL_ANDROID": "ca-app-pub-6862900859746528/5553637906",
        "EXPO_PUBLIC_ADMOB_INTERSTITIAL_IOS": "ca-app-pub-xxxxxxxxxxxxxxxx/yyyyyyyyyy"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "internal"
      },
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "your-app-store-connect-app-id",
        "appleTeamId": "your-team-id"
      }
    }
  }
}
